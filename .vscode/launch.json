{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "autoAttachChildProcesses": true,
      "name": "ellie @ spec",
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceRoot}/bin/ellie.js",
      "args": ["./demo/test/demo.conf.js", "--debugEnabled", "--specs", "${file}", "--logLevel", "debug"]
    },
    {
      "name": "ellie @ launch demo app",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceRoot}/demo/app/index.html",
      "runtimeArgs": ["--args", "--disable-web-security", "--incognito", "-â€“allow-file-access-from-files"]
    },
    {
      "name": "ellie @ attach demo app",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}/demo/app"
    }
  ]
}
