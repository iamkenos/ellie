{
  "cucumberautocomplete.steps": ["src/core/steps/definitions/*.ts", "./**/definitions/**/*.js"],
  "cucumberautocomplete.formatConfOverride": { "And": 3, "But": 3 },
  "cucumberautocomplete.onTypeFormat": true,
  "cucumberautocomplete.stepsInvariants": true,
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [{ "language": "javascript", "autoFix": true }, { "language": "typescript", "autoFix": true }],
  "files.associations": { "*.ejs": "javascript" },
  "[feature]": { "editor.defaultFormatter": "alexkrechik.cucumberautocomplete" },
  "[javascript]": { "editor.formatOnSave": false },
  "[typescript]": { "editor.formatOnSave": false }
}
