{
  "name": "@iamkenos/ellie",
  "version": "2.0.4",
  "license": "MIT",
  "author": "iamkenos",
  "description": "An abstraction of the popular WebdriverIO test automation framework",
  "main": "./build/index.js",
  "bin": {
    "ellie": "bin/ellie.js"
  },
  "homepage": "https://github.com/iamkenos/ellie#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iamkenos/ellie.git"
  },
  "bugs": {
    "url": "https://github.com/iamkenos/ellie/issues"
  },
  "scripts": {
    "start": "npm run build && node bin/ellie",
    "prebuild": "rimraf build && rimraf *ellie-*.tgz && npm run lint -- --fix",
    "build": "tsc --build tsconfig.build.json",
    "postbuild": "run-s postbuild:**",
    "postbuild:cp:types": "shx mkdir -p build/types && shx cp -R ./src/types build",
    "postbuild:cp:resources": "shx cp -R ./src/cli/resources build/cli",
    "postbuild:cp:prettierrc": "shx cp ./.prettierrc build/cli/resources",
    "postbuild:mv:tsconfig": "shx mv ./build/cli/resources/tsconfig.tpl.ejs ./build/cli/resources/tsconfig.json",
    "postbuild:rm:clutter": "rimraf ./build/cli/**/{.comparable,.reports,wdio.conf.js}",
    "demo:serve": "http-server ./demo/app -s",
    "demo:test": "./bin/ellie.js demo/test/demo.conf.ts demo/test/overrides.conf.js",
    "demo:test:bs": "./bin/ellie.js demo/test/demo.browserstack.conf.ts",
    "pack": "npm run build && npm pack",
    "lint": "eslint ./",
    "test": "run-s test:**",
    "test:unit": "jest --config .jestrc.js --coverage",
    "test:demo": "start-test demo:serve 8080 demo:test",
    "test:demo:bs": "start-test demo:serve 8080 demo:test:bs",
    "test:samples": "./bin/ellie.js src/cli/resources/samples/ellie.conf.ts",
    "release": "npm run build && node ./.scripts/s_release.js",
    "prepare": "husky install && npm run build"
  },
  "dependencies": {
    "@types/luxon": "2.0.0",
    "@wdio/allure-reporter": "7.9.1",
    "@wdio/browserstack-service": "7.9.1",
    "@wdio/cli": "7.9.1",
    "@wdio/cucumber-framework": "7.9.1",
    "@wdio/junit-reporter": "7.9.0",
    "@wdio/local-runner": "7.9.1",
    "@wdio/selenium-standalone-service": "7.8.0",
    "@wdio/spec-reporter": "7.9.0",
    "@wdio/sync": "7.9.1",
    "allure-commandline": "2.13.8",
    "chalk": "4.1.2",
    "deep-diff": "1.0.2",
    "ejs": "3.1.6",
    "fs-extra": "10.0.0",
    "inquirer": "8.1.2",
    "jsonpath": "1.1.1",
    "loglevel": "1.7.1",
    "loglevel-plugin-prefix": "0.8.4",
    "luxon": "2.5.2",
    "prettier": "2.3.2",
    "query-string": "7.0.1",
    "syncrequest": "1.4.7",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5",
    "wdio-image-comparison-service": "2.3.0",
    "wdio-intercept-service": "4.1.7",
    "yargs": "17.1.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/deep-diff": "1.0.1",
    "@types/ejs": "3.0.7",
    "@types/eslint": "7.28.0",
    "@types/fs-extra": "9.0.12",
    "@types/glob": "7.1.4",
    "@types/inquirer": "7.3.3",
    "@types/jest": "27.0.0",
    "@types/jsonpath": "0.2.0",
    "@types/lodash": "4.14.172",
    "@types/prettier": "2.3.2",
    "@types/request": "2.48.7",
    "@types/rimraf": "3.0.1",
    "@types/yargs": "17.0.2",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-wdio": "7.4.2",
    "http-server": "13.0.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "shx": "0.3.3",
    "start-server-and-test": "1.13.1",
    "ts-jest": "27.0.4"
  }
}
