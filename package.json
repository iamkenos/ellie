{
  "name": "ellie",
  "version": "1.0.0",
  "license": "ISC",
  "author": "iamkenos",
  "description": "A web app testing framework build on top of WebDriverIO and Cucumber",
  "main": "./build/index.js",
  "bin": {
    "ellie": "bin/ellie.js"
  },
  "homepage": "https://github.com/iamkenos/ellie#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iamkenos/ellie.git"
  },
  "bugs": {
    "url": "https://github.com/iamkenos/ellie/issues"
  },
  "scripts": {
    "start": "npm run build && node bin/ellie",
    "prebuild": "rimraf build && npm run lint -- --fix",
    "build": "tsc",
    "postbuild": "npm run postbuild:templates && npm run postbuild:config && npm run postbuild:pretty",
    "postbuild:templates": "copyfiles -u 1 \"./src/**/templates/**\" build",
    "postbuild:config": "copyfiles \"./.prettierrc\" build/cli/templates",
    "postbuild:pretty": "prettier --write \"./build/**/*.js\"",
    "debug": "node --nolazy --inspect-brk=9229 ./bin/ellie.js --",
    "demo:serve": "node ./node_modules/.bin/http-server -p22676 ./demo/app",
    "demo:test": "./bin/ellie.js demo/test/demo.conf.js",
    "demo:test:bs": "./bin/ellie.js demo/test/demo.bs.conf.js",
    "demo:report": "allure generate demo/test/.reports/allure -c -o demo/test/.reports/allure/report",
    "lint": "eslint ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.4",
    "@wdio/allure-reporter": "^5.16.16",
    "@wdio/browserstack-service": "^5.16.10",
    "@wdio/cli": "^5.16.16",
    "@wdio/cucumber-framework": "^5.16.15",
    "@wdio/junit-reporter": "^5.16.11",
    "@wdio/local-runner": "^5.16.15",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.16.15",
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "deep-diff": "^1.0.2",
    "ejs": "^3.0.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "syncrequest": "^1.4.7",
    "wdio-image-comparison-service": "^1.9.0",
    "wdio-intercept-service": "^3.0.3",
    "webdriverio": "^5.16.15",
    "winston": "^3.2.1",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/cucumber": "^6.0.0",
    "@types/deep-diff": "^1.0.0",
    "@types/ejs": "^3.0.0",
    "@types/eslint": "^6.1.3",
    "@types/fs-extra": "^8.0.1",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/prettier": "^1.19.0",
    "@types/request": "^2.48.4",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "copyfiles": "^2.1.1",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-wdio": "^5.13.2",
    "http-server": "^0.12.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "typescript": "^3.7.3"
  }
}
