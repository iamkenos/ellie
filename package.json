{
  "name": "@iamkenos/ellie",
  "version": "1.4.0",
  "license": "MIT",
  "author": "iamkenos",
  "description": "An abstraction of the popular WebdriverIO test automation framework",
  "main": "./build/index.js",
  "bin": {
    "ellie": "bin/ellie.js"
  },
  "homepage": "https://github.com/iamkenos/ellie#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iamkenos/ellie.git"
  },
  "bugs": {
    "url": "https://github.com/iamkenos/ellie/issues"
  },
  "scripts": {
    "start": "npm run build && node bin/ellie",
    "prebuild": "rimraf build && npm run lint -- --fix",
    "build": "tsc",
    "postbuild": "run-s postbuild:**",
    "postbuild:cp:resources": "copyfiles -u 1 \"./src/**/resources/**\" build",
    "postbuild:cp:prettierrc": "copyfiles \"./.prettierrc\" build/cli/resources",
    "demo:serve": "http-server ./demo/app -s",
    "demo:test": "./bin/ellie.js demo/test/demo.conf.ts demo/test/overrides.conf.js",
    "demo:test:bs": "./bin/ellie.js demo/test/demo.browserstack.conf.ts",
    "pack": "npm run build && rimraf *ellie-*.tgz && npm pack",
    "lint": "eslint ./",
    "test": "run-s test:**",
    "test:demo": "start-test demo:serve 8080 demo:test",
    "test:demo:bs": "start-test demo:serve 8080 demo:test:bs",
    "test:samples": "./bin/ellie.js src/cli/resources/samples/ellie.conf.ts",
    "test:unit": "jest --config .jestrc.json --coverage",
    "release": "npm run build && node ./.scripts/s_release.js"
  },
  "dependencies": {
    "@types/cucumber": "6.0.1",
    "@wdio/allure-reporter": "6.7.2",
    "@wdio/browserstack-service": "6.7.3",
    "@wdio/cli": "6.7.3",
    "@wdio/cucumber-framework": "6.7.3",
    "@wdio/junit-reporter": "6.7.0",
    "@wdio/local-runner": "6.7.3",
    "@wdio/selenium-standalone-service": "6.7.3",
    "@wdio/spec-reporter": "6.7.0",
    "@wdio/sync": "6.7.3",
    "allure-commandline": "2.13.0",
    "chai": "4.2.0",
    "chalk": "4.1.0",
    "deep-diff": "1.0.2",
    "ejs": "3.1.5",
    "fs-extra": "9.0.1",
    "inquirer": "7.3.3",
    "jsonpath": "1.0.2",
    "loglevel": "1.7.0",
    "loglevel-plugin-prefix": "0.8.4",
    "prettier": "2.1.2",
    "query-string": "6.13.7",
    "syncrequest": "1.4.7",
    "table": "6.0.3",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.5",
    "wdio-image-comparison-service": "2.0.0",
    "wdio-intercept-service": "4.1.2",
    "webdriverio": "6.7.3",
    "yargs": "16.1.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/deep-diff": "1.0.0",
    "@types/ejs": "3.0.5",
    "@types/eslint": "7.2.6",
    "@types/fs-extra": "9.0.6",
    "@types/glob": "7.1.3",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.20",
    "@types/jsonpath": "0.2.0",
    "@types/lodash": "4.14.168",
    "@types/prettier": "2.1.6",
    "@types/request": "2.48.5",
    "@types/rimraf": "3.0.0",
    "@types/yargs": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "copyfiles": "2.4.1",
    "dotenv": "^8.2.0",
    "eslint": "7.13.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.2",
    "eslint-plugin-wdio": "6.6.0",
    "http-server": "0.12.3",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "start-server-and-test": "1.12.0",
    "ts-jest": "26.4.4"
  }
}
