{
  "name": "@iamkenos/ellie",
  "version": "1.0.2",
  "license": "MIT",
  "author": "iamkenos",
  "description": "This is a thin wrapper around the popular WebdriverIO test automation framework",
  "main": "./build/index.js",
  "bin": {
    "ellie": "bin/ellie.js"
  },
  "homepage": "https://github.com/iamkenos/ellie#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iamkenos/ellie.git"
  },
  "bugs": {
    "url": "https://github.com/iamkenos/ellie/issues"
  },
  "scripts": {
    "start": "npm run build && node bin/ellie",
    "prebuild": "rimraf build && npm run lint -- --fix",
    "build": "tsc",
    "postbuild": "npm run postbuild:cp:resources && npm run postbuild:cp:prettierrc && npm run postbuild:prettier",
    "postbuild:cp:resources": "copyfiles -u 1 \"./src/**/resources/**\" build",
    "postbuild:cp:prettierrc": "copyfiles \"./.prettierrc\" build/cli/resources",
    "postbuild:prettier": "prettier --write \"./build/**/*.js\"",
    "demo:serve": "http-server ./demo/app",
    "demo:test": "./bin/ellie.js demo/test/demo.conf.ts",
    "postdemo:test": "npm run postdemo:report:gen",
    "postdemo:report:gen": "allure generate demo/test/.reports/allure -c -o demo/test/.reports/allure/report",
    "postdemo:report:serve": "allure open demo/test/.reports/allure/report",
    "pack": "rimraf *ellie-*.tgz && npm pack",
    "lint": "eslint ./",
    "test:functional": "start-test demo:serve 8080 demo:test",
    "test:unit": "jest --config .jestrc.json --coverage",
    "prepublish": "npm run build",
    "postpublish": "npm version patch && git push origin $(git rev-parse --abbrev-ref HEAD)"
  },
  "dependencies": {
    "@types/cucumber": "^6.0.0",
    "@wdio/allure-reporter": "^5.18.6",
    "@wdio/browserstack-service": "^5.18.1",
    "@wdio/cli": "^5.18.6",
    "@wdio/cucumber-framework": "^5.18.6",
    "@wdio/junit-reporter": "^5.18.6",
    "@wdio/local-runner": "^5.18.6",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.18.6",
    "@wdio/sync": "^5.18.6",
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "deep-diff": "^1.0.2",
    "ejs": "^3.0.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.4",
    "loglevel": "^1.6.6",
    "loglevel-plugin-prefix": "^0.8.4",
    "prettier": "^1.19.1",
    "syncrequest": "^1.4.7",
    "table": "^5.4.6",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "wdio-image-comparison-service": "^1.9.0",
    "wdio-intercept-service": "^4.0.0",
    "webdriverio": "^5.18.6",
    "yargs": "^15.1.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/deep-diff": "^1.0.0",
    "@types/ejs": "^3.0.0",
    "@types/eslint": "^6.1.7",
    "@types/fs-extra": "^8.0.1",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/prettier": "^1.19.0",
    "@types/request": "^2.48.4",
    "@types/rimraf": "^2.0.3",
    "@types/yargs": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-wdio": "^5.13.2",
    "http-server": "^0.12.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.2",
    "rimraf": "^3.0.1",
    "start-server-and-test": "^1.10.8",
    "ts-jest": "^25.1.0"
  }
}
